<body scroll="no">
<HTC:Menu id="Menu" xml="./System/Menu.xml" />
<div id="divState" align="right">
	<span id="playState"></span>　
	<span id="timeleft"></span>
</div>
<HTC:Tab id="Tab" selectedIndex="1">
	<span>リクエスト</span>
	<span>コメント</span>
	<span>動画情報</span>
	<span>連続コメント</span>
	<span>副管理者</span>
	<span>オプション</span>
</HTC:Tab>
<div id="TabContents">
	<div id="RequestForm">
		&nbsp;
		<!-- buttons -->
		<input id="btnPF" type="image" src="./System/assets/play_s.png" value="4" class="head-control-image" onclick="playFirst()" title="一番上を再生">
<!--add start-->
		<input type="image" src="./System/assets/stop_s.png" value="g" class="head-control-image" onclick="NicoLive.postComment('/stop')" title="停止">
		<input type="image" src="./System/assets/soundonly_s.png" value="7" class="head-control-image" onclick="NicoLive.postComment('/soundonly on')" title="音のみに切り替える">
<!--add end-->
		<input type="image" src="./System/assets/close_s.png" value="r" class="head-control-image" onclick="delFirst()" title="一番上を削除">
		<input id="btnPR" type="image" src="./System/assets/random_s.png" value="s" class="head-control-image" onclick="playRandom()" title="ランダム再生">

		<!-- extras -->
<!--
		<input type="checkbox" id="playSound" style="display: none"><label for="playSound" style="display: none">音のみ</label>
		<span id="divPlaySub"><input type="checkbox" id="playSub" style="display: none"><label for="playSub" style="display: none">小画面</label></span>
		<input id="btnCR" type="button" value="募集" onclick="changeRequest()" style="display: none">
		<input type="checkbox" id="extraComment" style="display: none">
-->
		<input type="hidden" id="playSound">
		<span id="divPlaySub"></span>
		<input type="hidden" id="playSub">
		<input type="hidden" id="btnCR">
		<input type="hidden" id="extraComment">
<!--add start-->
<!--
		<input type="button" value="１枠延長" onclick="extend()" style="display: none" />
		<input type="button" onclick="LimitTimeReset()" value="リセット" style="display: none" />
-->
<!--add end-->
<!--add start-->
	<span id="PlayJingleForm"></span>
		 <script>
			if(settings["jingles"]){
//				document.getElementById("PlayJingleForm").innerHTML = "ジングル ";
				var sel = document.createElement("select");
				sel.setAttribute("id", "jingle");
				for(var i=0,l=settings["jingles"].length;i<l;i++){
					if(settings["jingles"][i]!=""){
						var opt = document.createElement("option");
						if( typeof(settings["jingles"][i]) == 'object' ){
							opt.appendChild(document.createTextNode(settings["jingles"][i][1]));
							opt.setAttribute("value", settings["jingles"][i][0]);
						}
						else{
							opt.appendChild(document.createTextNode(settings["jingles"][i]));
							opt.setAttribute("value", settings["jingles"][i]);
						}
						sel.appendChild(opt);
					}
				}
				document.getElementById("PlayJingleForm").appendChild(sel);
				document.getElementById("PlayJingleForm").innerHTML += " <input type=\"image\" src=\"./System/assets/play_jingle_s.png\" class=\"head-control-image\" onclick=\"NicoLive.postComment(document.getElementById('jingle').value)\" title=\"ジングルを再生\" />";
			}
		 </script>
<!--add end-->
		<hr>
	</div>
	<div id="RequestHTML"></div>
</div>
<div id="TabContents">
<!--add start-->
	<input type="text" id="sendCmt_mail" size="10" />
	<input type="text" id="sendCmt_text" size="50" />
	<input type="button" value="主コメする" onclick="NicoLive.postComment(document.getElementById('sendCmt_text').value, document.getElementById('sendCmt_mail').value);document.getElementById('sendCmt_text').value='';document.getElementById('sendCmt_mail').value='';" />
	<hr />
<!--add end-->
	<div id="CommentHTML"></div>
</div>
<div id="TabContents">
	<div id="VideoInformation">
	</div>
</div>
<!--add start-->
<div id="TabContents">
	<div id="AutoComment">
		<div id="divAutoPlay">
			<fieldset>
				<legend>主コメント</legend>
				<input id="AutoCmtBtn" type="button" value="1行出力" onclick="AutoCmtOnePut()"><br><br>
				<input id="AutoCmAlltBtn" type="button" value="連続出力" onclick="AutoCmtAllPut()">
				コメント間隔:<input id="AutoCmtInterval" type="text" size="3" value="5">秒<br><br>
				内容：<br><textarea name="AutoCmtText" rows="10" cols="50"></textarea>
			</fieldset>
		</div>
	</div>
</div>
<!--add end-->
<!--add start-->
<div id="TabContents">
	<div id="dummyAdmin">
		<fieldset>
			<legend>副管理者登録</legend>
			<input type="checkbox" id="dummyAdminAddMsgCheck" checked><label for="dummyAdminAddMsgCheck">登録時に告知表示</label>
			<input type="checkbox" id="dummyAdminChgMsgCheck" checked><label for="dummyAdminChgMsgCheck"> 変更時に告知表示</label>
			<input type="checkbox" id="dummyAdminDelMsgCheck" checked><label for="dummyAdminDelMsgCheck"> 削除時に告知表示</label>
			<br><br>
			<input type="checkbox" id="dummyAdminCmdCheck"><label for="dummyAdminCmdCheck">コマンド </label>
			<input type="checkbox" id="dummyAdminCmtCheck"><label for="dummyAdminCmtCheck">コメント </label>
			<input type="checkbox" id="dummyAdminNGCheck" checked><label for="dummyAdminNGCheck">NGリストに追加</label>
			<input type="checkbox" id="dummyAdminNameCheck" checked><label for="dummyAdminNameCheck">名前の表示</label><br>
			<label>名前：</label><input id="dummyAdminName" type="text" size="6" value=""><label> ID：</label><input id="dummyAdminID" type="text" size="32" value=""><label> ←コメ番</label><input id="dummyAdminNo" type="text" size="4" value=""><input type="button" value="取得" onclick="GetNotoID()"><br>
			<input type="button" value="↓追加↓" onclick="dummyAdminAdd()">
		</fieldset>
	</div>
	<hr>
	<div id="dumyAdminHTML"></div>
</div>
<!--add end-->
<div id="TabContents">
	<div id="OptionForm">
		<div id="divAutoPlay">
		<fieldset>
			<legend>自動再生</legend>
			<input type="checkbox" id="autoPlay" onclick="checkAutoPlay(this.checked)"><label for="autoPlay">自動再生</label>/再生間隔:<input id="autoPlayInterval" type="text" size="3" value="60000">秒/<input type="checkbox" id="autoPlayRandom"><label for="autoPlayRandom">ランダム再生</label><br>
			<input type="checkbox" id="autoPlayStanby"><label for="autoPlayStanby">ストック数0の場合は待機する</label>
		</fieldset>
		</div>
		<div id="divSofTalk">
		<fieldset>
			<legend>SofTalk</legend>
			<input type="checkbox" id="softalk"><label for="softalk">コメント読み上げ</label>
		</fieldset>
		</div>
		<div id="divLoginCheck">
		<fieldset>
			<legend>接続/切断</legend>
			<input type="checkbox" id="loginCheck" onclick="checkloginCheck()"><label for="loginCheck">ログオフ時強制ログイン</label>
<!--del			<input type="checkbox" id="logoffCheck" checked=true onclick="checklogoffCheck()"><label for="logoffCheck">30分強制ログアウト</label>-->
<!--add start-->
			<input type="checkbox" id="logoffCheck" onclick="checklogoffCheck()"><label for="logoffCheck">30分強制ログアウト</label>
<!--add end-->
		</fieldset>
		</div>

	</div>
</div>
<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" id="Socket" style="width:0px;height:0px;">
